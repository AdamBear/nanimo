<?xml version="1.0" encoding="UTF-8"?>

<ccg-lexicon name="fluffy">

  <family name="A" pos="noun">
    <entry name="Primary">
      <atomcat type="N"/>
    </entry>
  </family>

  <family name="B" pos="intransitive">
    <entry name="Primary">
      <complexcat>
        <atomcat type="S"/>
        <slash dir="\"/>
        <atomcat type="N"/>
      </complexcat>
    </entry>
  </family>

  <family name="C" pos="transitive">
    <entry name="Primary">
      <complexcat>
        <atomcat type="S"/>
        <slash dir="\"/>
        <atomcat type="N"/>
        <slash dir="/"/>
        <atomcat type="N"/>
      </complexcat>
    </entry>
  </family>

  <family name="D" pos="reflexive">
    <entry name="Primary">
      <complexcat>
        <atomcat type="S"/>
        <slash dir="\"/>
        <atomcat type="N"/>
        <slash dir="\"/>
	<complexcat>
	  <atomcat type="S"/>
	  <slash dir="\"/>
	  <atomcat type="N"/>
	  <slash dir="/"/>
	  <atomcat type="N"/>
	</complexcat>
      </complexcat>
    </entry>
  </family>

</ccg-lexicon>